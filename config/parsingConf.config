server {
	listen 8045;
	server_name : Stas42;
	client_max_body_size 10M

	location / {	
		root www;
		index static/index.html;
		autoindex on;
		allowed_methods GET POST;
	}

	error_page 400 static/bad_request.html;
	error_page 404 static/404.html;
	error_page 405 static/method_not_allowed.html;
	error_page 413 static/413.html;
	error_page 500 static/500.html;
}
