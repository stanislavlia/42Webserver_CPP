server {
	listen 8012;
	server_name : Stas42;
	client_max_body_size 10M

	location / {	
		root /www;
		index static/index.html;
		autoindex on;
		allowed_methods POST;
	}

	error_page 400 ./static/bad_request.html;
	error_page 404 /404.html;
	error_page 405 ./static/method_not_allowed.html;
	error_page 413 /413.html;
	error_page 500 /500.html;
}
